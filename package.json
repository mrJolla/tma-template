  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prod": "yarn && yarn test:all:fix && yarn build && yarn preview",
    "ts-check": "tsc --noEmit",
    "knip": "knip",
    "test:all": "yarn ts-check && yarn test:eslint && yarn test:stylelint",
